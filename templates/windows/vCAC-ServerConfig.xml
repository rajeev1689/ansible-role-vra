    <DATABASE_INSTALL_SQL_USER_PASSWORD></DATABASE_INSTALL_SQL_USER_PASSWORD>
    <PRECREATED_DATABASE_FLAG>False</PRECREATED_DATABASE_FLAG>
    <CREATE_MSSQL_AZMAN_STORE_FLAG>false</CREATE_MSSQL_AZMAN_STORE_FLAG>
  </database>
